    {
        "id": "event_8",
        "type": "party",
        "name": "Makrovoutes Closing Party at University of Crete",
        "artists": [
            "V.V.I.A. (Venus Volcanism + In Atlas)",
            "krats",
            "Screw Operation",
            "Ouroboros / Dj Office",
